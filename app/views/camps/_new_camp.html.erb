<%= form_for camp, :html => {:id => 'new_camp_form'} do |f| %>
  <%= f.label :name, "Camp Name" %>
  <br>
  <%= f.text_field :name %>
  <br>
  <br>
  <%= f.label :location, "Location" %>
  <br>
  <%= f.text_field :location %>
  <br>
  <br>
  <%= f.label :description, "Description" %>
  <br>
  <%= f.text_area :description, size: "50x10" %>
  <br>
  <br>
  <%= f.submit "Add Camp" %>
<% end %>
